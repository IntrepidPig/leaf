fn main() {
	let a := 20;
	let b := get_2000() + a;
	let c := count_to_3000() + b;
	let d := add_42(c);
	sum(sum(c, d), a)
}

fn get_2000() {
	let d := 1000;
	let e := 1000;
	d + e
}

fn count_to_3000() {
	let f := 0;
	loop
		if f == 3000 then {
			break;
		} else {
			f := f + 1000;
		} end;
	back;
	f
}

fn add_42(x) {
	x + 42
}

fn sum(a, b) {
	let sum := a + b;
	sum
}
